    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python Odoo v16",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/odoo16/odoo/odoo-bin",
        "python": "${workspaceFolder}/odoo16/env/bin/python",
        "console": "integratedTerminal",
        "args": ["-c","${workspaceFolder}/odoo16/odoo.conf","-u","dclick_sale",],
        "stopOnEntry": false,
        "logToFile": true
      },
      // , "-u", "all", "1010_advance_payments_dual_currency"
  
      // "--auto-reload",
      // "--debug",
     
      {
          "type": "chrome",
          "request": "launch",
          "name": "JS Odoo v16",
          "url": "http://localhost:8069/web?debug=assets",
          "webRoot": "${workspaceFolder}/odoo16/odoo",
          "sourceMapPathOverrides":{
              "*":"${workspaceFolder}/odoo16/odoo/*",
          }
      },
    ]
  }
  